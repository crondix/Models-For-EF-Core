@page "/"
@using BlazorApp1.Data
@using MyModels.Models
@inject ApplicationContext DbContextPrinters

<PageTitle>Index</PageTitle>

@foreach(var printer in printers)
{
    <BlazorApp1.Components.Layout.Printer_Card Printer="@printer"></BlazorApp1.Components.Layout.Printer_Card>
}


@code{
   
    public virtual ICollection<Printers> printers { get; set; }
    protected override void OnInitialized()
    {
         printers = DbContextPrinters.Printers.ToList();
        // printers = DbContextPrinters.Printers.ToList();
    }
}
